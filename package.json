{
  "name": "react-router-relative-link",
  "version": "0.1.8",
  "description": "A wrapper around react-router's Link and NavLink that allows relative paths.",
  "dependencies": {
    "react": "^16.12.0",
    "react-router-dom": "^5.1.2"
  },
  "keywords": [
    "react",
    "react-router",
    "react-router-dom",
    "relative",
    "path"
  ],
  "homepage": "https://github.com/shhhplus",
  "main": "index.js",
  "author": {
    "name": "shhhplus"
  },
  "bugs": {
    "url": "https://github.com/shhhplus/react-router-relative-link/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shhhplus/react-router-relative-link.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:package": "node ./scripts/build/index.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.2",
    "@storybook/addon-knobs": "^5.3.2",
    "@storybook/addon-links": "^5.3.2",
    "@storybook/addons": "^5.3.2",
    "@storybook/react": "^5.3.2",
    "husky": "^4.0.7",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "react-scripts": "3.3.0",
    "react-styleguidist": "^10.5.0",
    "rollup": "^1.29.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
